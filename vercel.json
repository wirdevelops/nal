{
    "version": 2,
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "NODE_ENV": "production"
    },
    "build": {
      "env": {
        "NEXT_TELEMETRY_DISABLED": "1"
      }
    },
    "functions": {
      "api/**/*.ts": {
        "memory": 3008,
        "maxDuration": 60
      }
    }
  }